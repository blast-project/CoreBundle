{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}

    {% set dest_toolbar = constant('Blast\\Bundle\\CoreBundle\\Profiler\\DataCollection::DESTINATION_TOOLBAR') %}
    {% set dest_profiler = constant('Blast\\Bundle\\CoreBundle\\Profiler\\DataCollection::DESTINATION_PROFILER') %}
    {% set dest_both = constant('Blast\\Bundle\\CoreBundle\\Profiler\\DataCollection::DESTINATION_BOTH') %}

    {% set icon %}
        <span class="icon">
            <img
                alt="Blast" src="data:image/png;base64,"
                style="height: 26px;width: 30px;"
            />
        </span>
        <span class="sf-toolbar-label" style="position: relative;top: -3px;margin-left: 5px;">
            Blast
        </span>
    {% endset %}

    {% set text %}

    {% for k, d in collector.getData()[dest_toolbar] %}
        <div class="sf-toolbar-info-piece">
            {% for key, data in d %}
                {% if data.class is defined %}
                    <b>{{ key|capitalize }}</b>
                    {% if data.file is defined %}
                        <a href="{{data.file|file_link(1)}}">
                            {{ data.class }}
                        </a>
                    {% else %}
                        <span class="sf-toolbar-status">{{ data.class }}</span>
                    {% endif %}
                {% endif %}
                {% if data is iterable %}
                    {% for key, item in data if key not in ['class', 'file', 'display'] %}
                        <b>{{ key }}</b>
                        {{dump(item)}}
                    {% endfor %}
                {% endif %}
            {% endfor %}
        </div>
    {% endfor %}

    {% endset %}

    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { link: true }) }}
{% endblock %}

{% block head %}
    {{ parent() }}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon">
            <img
                alt="Blast" src="data:image/png;base64,"
                style="height: 26px;width: 30px;"
            />
        </span>
        <strong>Blast</strong>
    </span>
{% endblock %}

{% block panel %}

    {% set dest_toolbar = constant('Blast\\CoreBundle\\Profiler\\DataCollection::DESTINATION_TOOLBAR') %}
    {% set dest_profiler = constant('Blast\\CoreBundle\\Profiler\\DataCollection::DESTINATION_PROFILER') %}
    {% set dest_both = constant('Blast\\CoreBundle\\Profiler\\DataCollection::DESTINATION_BOTH') %}

    {% for k, d in collector.getData()[dest_profiler] %}
        <table>
            <thead>
                <tr>
                    <th colspan="100%">
                        {{ k }}
                    </th>
                </tr>
            </thead>
            <tbody>
                {% for key, data in d %}
                    {% if data.class is defined %}
                        <tr>
                            <td style="width:30%;text-align:right">
                                <b>{{ key|capitalize }}</b>
                            </td>
                            <td>
                                {% if data.file is defined %}
                                    <a href="{{data.file|file_link(1)}}">
                                        {{ data.class }}
                                    </a>
                                {% else %}
                                    {{ data.class }}
                                {% endif %}
                            </td>
                        </tr>
                    {% endif %}
                    {% if data is iterable %}
                        {% for key, item in data if key not in ['class', 'file', 'display'] %}
                            <tr>
                                <td style="width:30%;text-align:right">
                                    <b>{{ key }}</b>
                                </td>
                                <td>
                                    {{dump(item)}}
                                </td>
                            </tr>
                        {% endfor %}
                    {% endif %}

                {% endfor %}
            </tbody>
        </table>
    {% endfor %}
{% endblock %}
